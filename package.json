    "@babel/runtime-corejs3": "^7.18.9",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@types/jest": "^29.2.2",
    "@types/react": "^18.0.10",
    "@types/react-dom": "^18.0.5",
    "@types/styled-components": "^5.1.26",
    "@typescript-eslint/eslint-plugin": "^5.33.0",
